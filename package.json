{
  "name": "missao-africa",
  "version": "1.0.0",
  "description": "Missao Africa",
  "author": "Daniel Rodrigues",
  "license": "ISC",
  "engines": {
    "yarn": "1.7.0"
  },
  "main": "src/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run serve --prefix client\"",
    "dev:server": "nodemon --exec babel-node src/index.js",
    "start": "NODE_ENV=production babel-node src/index.js",
    "postinstall": "npm run install:client --prefix client",
    "heroku-postbuild": "npm run build --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "concurrently": "^3.6.0",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "nodemon": "^1.14.12"
  }
}

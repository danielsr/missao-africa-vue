{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "no-tabs": "off",
    "consistent-return": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off"
  }
}